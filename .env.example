# Grafana Credentials
GRAFANA_USER=
GRAFANA_PASSWORD=

# Production Database Host
PROD_DB_HOST=host
PROD_DB_PORT=5432

# Production - Main Database (neondb)
PROD_MAIN_DB_NAME=neondb
PROD_MAIN_DB_USER=prod_main_user
PROD_MAIN_DB_PASSWORD=prod_main_secure_password

# Production - User Database (userdb)  
PROD_USER_DB_NAME=userdb
PROD_USER_DB_USER=prod_user_user
PROD_USER_DB_PASSWORD=prod_user_secure_password

# Development Database
DEV_DB_HOST=localhost
DEV_DB_PORT=5432
DEV_DB_NAME=dev_database
DEV_DB_USER=postgres
DEV_DB_PASSWORD=dev_password

# Security Note: In production, use Docker secrets or external secret management
# PROD_MAIN_DB_PASSWORD_FILE=/run/secrets/prod_main_db_password
# PROD_USER_DB_PASSWORD_FILE=/run/secrets/prod_user_db_password